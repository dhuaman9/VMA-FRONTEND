  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4300",
    "start:dev:with:env": "ng serve --configuration developmentWithEnv --port 4501",
    "start:qa": "ng serve --configuration quality --port 4501",
    "start-with-proxy": "ng serve --proxy-config proxy.conf.json",
